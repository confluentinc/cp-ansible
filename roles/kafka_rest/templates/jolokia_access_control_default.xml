<?xml version="1.0" encoding="utf-8"?>
<restrict>
  <commands>
    <command>read</command>
  </commands>
  <allow>
    <mbean>
      <name>kafka.rest:type=*</name>
    </mbean>
    <mbean>
      <name>io.confluent.kafkarest:type=*</name>
    </mbean>
  </allow>
  <deny>
    <mbean>
      <name>*:*</name>
    </mbean>
  </deny>
</restrict>
